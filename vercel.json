{
  "version": 2,
  "functions": {
    "api/*.py": {
      "runtime": "python3.9"
    }
  },
  "routes": [
    { "src": "/api/(.*)", "dest": "/api/$1" },
    { "src": "/static/(.*)", "dest": "/static/$1" },
    { "src": "/(.*)", "dest": "/api/index" }
  ],
  "env": {
    "PYTHONPATH": "./"
  },
  "build": {
    "env": {
      "PYTHONPATH": "./"
    }
  }
}
