{
  "version": 2,
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python"
    },
    {
      "src": "static/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/index.py"
    },
    {
      "src": "/dashboard",
      "dest": "/api/index.py"
    },
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/api/index.py"
    }
  ],  "env": {
    "SECRET_KEY": "@secret_key",
    "MT5_API_KEY": "@mt5_api_key", 
    "ALLOWED_EMAILS": "@allowed_emails",
    "ACCESS_CODE": "@access_code",
    "ALLOWED_IPS": "@allowed_ips"
  }
}