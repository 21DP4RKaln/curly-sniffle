{
  "version": 2,
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python"
    },
    {
      "src": "static/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/index.py"
    },
    {
      "src": "/dashboard",
      "dest": "/static/dashboard.html"
    },
    {
      "src": "/login",
      "dest": "/static/auth.html"
    },
    {
      "src": "/auth",
      "dest": "/static/auth.html"
    },
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/",
      "dest": "/static/dashboard.html"
    },
    {
      "src": "/(.*)",
      "dest": "/static/dashboard.html"
    }
  ],
  "env": {
    "FLASK_ENV": "production"
  },
  "functions": {
    "api/index.py": {
      "maxDuration": 30
    }
  }
}