{
  "name": "svn-trading-bot",
  "version": "1.0.0",
  "description": "SVN Smart Money Trading Bot with AI - MT5 Integration",
  "main": "api/index.py",
  "scripts": {
    "dev": "python -m api.index",
    "start": "python -m api.index",
    "build": "echo 'No build step required for Python serverless functions'",
    "deploy": "vercel --prod",
    "deploy:dev": "vercel dev",
    "setup": "deploy.bat",
    "migrate": "python -c 'from api.database import init_db; init_db()'",
    "clean": "rmdir /s /q __pycache__ 2>nul || echo 'Cache cleaned'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yourusername/svn-trading-bot.git"
  },
  "keywords": [
    "trading",
    "ai",
    "bot", 
    "vercel",
    "serverless",
    "python",
    "flask"
  ],
  "author": "SVN Team",
