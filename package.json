  "bugs": {
    "url": "https://github.com/yourusername/svn-trading-bot/issues"
  },
  "homepage": "https://github.com/yourusername/svn-trading-bot#readme",
  "devDependencies": {
    "vercel": "^32.0.0"
  },
  "engines": {
    "python": "3.9"
  }
}
